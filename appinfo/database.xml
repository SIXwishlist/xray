<?xml version="1.0" encoding="ISO-8859-1" ?>
<database>
	<name>*dbname*</name>
	<create>true</create>
	<overwrite>false</overwrite>
	<charset>utf8</charset>

	<table>
		<name>*dbprefix*xray_log</name>
		<declaration>
			<field>
				<name>id</name>
				<type>integer</type>
				<default>0</default>
				<notnull>true</notnull>
				<autoincrement>1</autoincrement>
				<length>4</length>
			</field>

			<field>
				<name>data</name>
				<type>text</type>
				<notnull>true</notnull>
				<length>10240</length>
			</field>
			<field>
				<name>request_id</name>
				<type>text</type>
				<notnull>true</notnull>
				<length>64</length>
			</field>
			<field>
				<name>timestamp</name>
				<type>integer</type>
				<notnull>true</notnull>
				<length>4</length>
			</field>
		</declaration>
	</table>
</database>
